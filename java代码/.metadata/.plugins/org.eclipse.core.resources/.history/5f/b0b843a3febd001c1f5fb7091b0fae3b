package 第一周代码;

import java.util.Scanner;

public class 代码03dowhile循环实例 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		double height = sc.nextDouble();
		int i = sc.nextInt();
		do {
			height = height * 2;
			i++;
		} while (height <= 8848);
		System.out.println("需要折叠" + i + "次");
	}
}
